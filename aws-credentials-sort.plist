<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>aws-credentials-sort.plist</string>
    <key>OnDemand</key>
    <true/>
    <key>ProgramArguments</key>
    <array>
        <string>/bin/sh</string>
        <string>-c</string>
        <string>/usr/bin/python3 $HOME/.local/bin/aws-credentials-sort.py</string>
    </array>
    <key>StandardErrorPath</key>
    <string>/tmp/aws-credentials-sort.error</string>
    <key>StandardOutPath</key>
    <string>/tmp/aws-credentials-sort.out</string>
    <key>StartInterval</key>
    <integer>1</integer>
    <key>KeepAlive</key>
    <true/>
    <key>RunAtLoad</key>
    <true/>
</dict>
</plist>